{
  "name": "painperdu",
  "version": "2.0.0",
  "description": "React Component to quickly navigate between all your routes",
  "keywords": [
    "react",
    "component",
    "routes",
    "tailwind",
    "react-router"
  ],
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Tostee Lucas",
    "email": "lucas.tostee@gmail.com"
  },
  "contributors": [
    {
      "name": "Joya Nguyen KÃ©vin",
      "email": "kevin.joya@outlook.com"
    }
  ],
  "repository": "github:luctst/painperdu",
  "bugs": {
    "url": "https://github.com/luctst/painperdu/issues"
  },
  "packageManager": "pnpm",
  "license": "MIT",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "lint": "eslint \"src/**/*.{ts, vue}\" --fix",
    "typecheck": "tsc",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/builder-vite": "^7.6.10",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/test": "^7.6.3",
    "@types/node": "^20.10.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "cssnano": "^6.1.0",
    "eslint": "^8.0.1",
    "eslint-config-painperdu": "workspace:^",
    "eslint-plugin-storybook": "^0.6.15",
    "storybook": "^7.6.3",
    "ts-config-painperdu": "workspace:^",
    "typescript": "*",
    "vite": "^5.0.12",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": ">=6.x"
  }
}
